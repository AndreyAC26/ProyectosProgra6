@model IEnumerable<Entidades.SalesReason>

@{
    ViewBag.Title = "listarSalesReason_ENT";
}

<h2>Lista de Motivos de Venta</h2>

<p>
    @Html.ActionLink("Nuevo", "agregarSalesReason_ENT")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SalesReasonID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModifiedDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SalesReasonID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModifiedDate)
        </td>
        <td>
            @Html.ActionLink("Modificar", "modificaSalesReason_ENT", new { pId=item.SalesReasonID }) |
            @Html.ActionLink("Detalles", "detalleSalesReason_ENT", new { pId=item.SalesReasonID }) |
            @Html.ActionLink("Borrar", "eliminarSalesReason_ENT", new { pId=item.SalesReasonID })
        </td>
    </tr>
}

</table>
