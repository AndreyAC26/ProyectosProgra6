@model IEnumerable<Entidades.Currency>
@{
    ViewBag.Title = "listarCurrency_ENT";
}

<h2>Listado de Divisas</h2>

<p>
    @Html.ActionLink("Nuevo", "insertarCurrency_ENT")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CurrencyCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModifiedDate)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CurrencyCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifiedDate)
            </td>
            <td>
                @Html.ActionLink("Editar", "modificarCurrency_ENT", new {  pId=item.CurrencyCode  }) |
                @Html.ActionLink("Detalles", "detalleCurrency_ENT", new { pId = item.CurrencyCode }) |
                @Html.ActionLink("Borrar", "eliminarCurrency_ENT", new { pId = item.CurrencyCode })
            </td>
        </tr>
    }

</table>
